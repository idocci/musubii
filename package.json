{
  "name": "musubii",
  "description": "Simple CSS Framework for JP",
  "version": "7.0.0",
  "main": "dist/musubii.css",
  "license": "MIT",
  "homepage": "https://musubii.qranoko.jp",
  "repository": "https://github.com/qrac/musubii",
  "bugs": "https://github.com/qrac/musubii/issues",
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework",
    "japanese",
    "musubii"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "dev": "next",
    "generate": "next build && next export",
    "build": "yarn build:sass && yarn build:postcss && yarn build:cleancss",
    "build:sass": "node-sass ./src/scss -o ./dist --include-path ./node_modules --output-style expanded",
    "build:postcss": "postcss ./dist/musubii.css -d ./dist --no-map",
    "build:cleancss": "cleancss -o ./dist/musubii.min.css ./dist/musubii.css -d",
    "extract": "yarn extract:sass && yarn extract:json && yarn extract:replace",
    "extract:sass": "bash ./tasks/extract-sass.sh",
    "extract:json": "bash ./tasks/extract-json.sh",
    "extract:replace": "bash ./tasks/extract-replace.sh",
    "clean": "rmtrash ./out ./next ./src/assets/extracts/.temp"
  },
  "browserslist": [
    "> 3% in JP",
    "ie 11",
    "android 4.4",
    "last 1 versions"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@mdx-js/loader": "^1.5.3",
    "@next/mdx": "^9.1.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.7.3",
    "babel-plugin-root-import": "^6.4.1",
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "cpx": "^1.5.0",
    "highlight.js": "^9.17.1",
    "js-beautify": "^1.10.2",
    "next": "^9.1.6",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "postcss-sort-media-queries": "^1.0.15",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "remark-custom-blocks": "^2.4.2",
    "remark-slug": "^5.1.2",
    "remark-toc": "^6.0.0",
    "replace": "^1.1.1",
    "sass-export": "^1.0.6"
  }
}
