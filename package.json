{
  "name": "musubii",
  "description": "Simple CSS Framework for JP",
  "version": "7.0.0",
  "main": "dist/musubii.css",
  "license": "MIT",
  "homepage": "https://musubii.qranoko.jp",
  "repository": "https://github.com/qrac/musubii",
  "bugs": "https://github.com/qrac/musubii/issues",
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework",
    "japanese",
    "musubii"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "dev": "docz dev",
    "generate": "docz build --dest ./public",
    "serve": "docz build --dest ./public && docz serve",
    "build": "yarn build:sass && yarn build:postcss && yarn build:cleancss",
    "build:sass": "node-sass ./src/scss -o ./dist --include-path ./node_modules --output-style expanded",
    "build:postcss": "postcss ./dist/musubii.css -d ./dist --no-map",
    "build:cleancss": "cleancss -o ./dist/musubii.min.css ./dist/musubii.css -d",
    "clean": "rmtrash ./public ./.docz"
  },
  "browserslist": [
    "> 3% in JP",
    "ie 11",
    "android 4.4",
    "last 1 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "clean-css-cli": "^4.3.0",
    "docz": "^2.1.1",
    "gatsby-plugin-sass": "^2.1.24",
    "js-beautify": "^1.10.2",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "postcss-sort-media-queries": "^1.0.9",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "react-syntax-highlighter": "^11.0.2"
  }
}
